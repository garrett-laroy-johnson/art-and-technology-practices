<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="description" content="" />
    <meta name="author" content="Garrett Johnson" />
    <title>{{ course }}</title>
    <style>
      .bd-placeholder-img {
        font-size: 1.125rem;
        text-anchor: middle;
        -webkit-user-select: none;
        -moz-user-select: none;
        user-select: none;
      }

      @media (min-width: 768px) {
        .bd-placeholder-img-lg {
          font-size: 3.5rem;
        }
      }
    </style>

    <link rel="stylesheet" href="/assets/css/bootstrap.css" />
    <!-- Custom styles -->
    <link href="/assets/css/sidebars.css" rel="stylesheet" />
    <link href="/assets/css/style.css" rel="stylesheet" />
    <script src="/assets/js/p5.js"></script>
    {# Google Fonts #}
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Noto+Serif&family=Silkscreen&display=swap"
      rel="stylesheet"
    />
    
  </head>
{# 
  <body onLoad="initDark()"> #}
    <body>
  

      <div class="row">
        <!-- sidebar -->
        <div class="col-sm-3" style="z-index: 2">
          <div class="flex-shrink-0 p-3" style="width: 100%">
            <h4 class="fs-5 fw-semibold" style="margin-left: 10px">
              {{ course }}
            </h4>
            <ul class="list-unstyled ps-0">
             {# course info #}
              {% for p in collections[memberCollection] %} 
              {% if p.data.weekNum == 0 %}

              <li class="mb-1">
                <a href="{{ p.url }}">
                  <button
                    class="btn align-items-left rounded collapsed nav-butt"
                    style="text-align: left"
                    aria-expanded="false"
                  >
                
                 {{ p.data.title }}
            
             
                 
                  </button>
                </a>
              </li>
              {% endif %}
              {% endfor %}
              {# weeks  #}
              {% for p in collections[memberCollection] %} 
              {% if p.data.weekNum > 0 %}

              <li class="mb-1">
                <a href="{{ p.url }}">
                  <button
                    class="btn align-items-left rounded collapsed nav-butt"
                    style="text-align: left"
                    aria-expanded="false"
                  >
                
                    Week {{ p.data.weekNum }} : {{ p.data.title }}
            
             
                 
                  </button>
                </a>
              </li>
              {% endif %}
              {% endfor %}
            
            </ul>
          </div>
        </div>
      
        {{content | safe }}
      
        </div>
      
      

    <script src="/assets/js/bootstrap.js" async defer></script>
    <script src="/assets/js/display.js" async defer></script>
  </body>
</html>
